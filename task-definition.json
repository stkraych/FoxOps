{
  "family": "service",
  "taskRoleArn": "",
  "executionRoleArn": "",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "node:latest",
      "memory": 800,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "command": ["yarn", "parcel", "src/index.html"],
      "workingDirectory": "/usr/src/app",
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tamago-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "app-container"
        }
      }
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["EC2"],
  "cpu": "",
  "memory": "",
  "tags": [],
  "pidMode": "task",
  "ipcMode": "task",
  "proxyConfiguration": null,
  "inferenceAccelerators": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "WINDOWS_SERVER_20H2_CORE"
  }
}
